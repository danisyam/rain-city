{
    "name": "Rain City Day",
    "type": "light",
    "colors": {
        "foreground": "#4a5568",
        "descriptionForeground": "#718096",
        "focusBorder": "#63b3ed33",
        "errorForeground": "#e53e3e",
        "widget.shadow": "#00000015",
        "scrollbar.shadow": "#00000015",
        "badge.background": "#63b3ed",
        "badge.foreground": "#ffffff",
        "icon.foreground": "#718096",
        "settings.headerForeground": "#4299e1",
        "window.activeBorder": "#e2e8f0",
        "window.inactiveBorder": "#edf2f7",
        "sash.hoverBorder": "#63b3ed",
        "toolbar.activeBackground": "#e2e8f0",
        "toolbar.hoverBackground": "#edf2f7",
        "extensionButton.prominentBackground": "#4299e1",
        "extensionButton.prominentHoverBackground": "#63b3ed",
        "extensionButton.prominentForeground": "#ffffff",
        "input.background": "#edf2f7",
        "input.foreground": "#4a5568",
        "activityBar.background": "#e7edf3",
        "sideBar.background": "#e7edf3",
        "statusBar.background": "#dae4ed",
        "statusBar.foreground": "#4a5568",
        "tab.activeBackground": "#edf2f7",
        "tab.activeForeground": "#2d3748",
        "list.activeSelectionBackground": "#e6f2ff",
        "list.hoverBackground": "#e7edf3",
        "editor.background": "#edf2f7",
        "editor.foreground": "#4a5568",
        "editor.selectionBackground": "#63b3ed40",
        "editor.lineHighlightBackground": "#e2e8f0",
        "editorCursor.foreground": "#4299e1",
        "editorWhitespace.foreground": "#cbd5e0",
        "editorIndentGuide.background1": "#d1dce8",
        "editorLineNumber.foreground": "#a0aec0",
        "editorLineNumber.activeForeground": "#4a5568"
    },
    "tokenColors": [
        {
            "scope": [
                "variable",
                "variable.parameter",
                "support.variable",
                "variable.other"
            ],
            "settings": {
                "foreground": "#2b6cb0"
            }
        },
        {
            "scope": [
                "keyword",
                "storage.type",
                "keyword.operator",
                "constant.language"
            ],
            "settings": {
                "foreground": "#5a3d9b"
            }
        },
        {
            "scope": [
                "function",
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "foreground": "#b7461a"
            }
        },
        {
            "scope": [
                "entity.name.tag",
                "meta.tag",
                "support.class.component"
            ],
            "settings": {
                "foreground": "#5a3d9b"
            }
        },
        {
            "scope": [
                "string",
                "string.quoted.double",
                "string.quoted.single"
            ],
            "settings": {
                "foreground": "#2a724f"
            }
        },
        {
            "scope": [
                "entity.other.attribute-name"
            ],
            "settings": {
                "foreground": "#b7461a"
            }
        },
        {
            "scope": [
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#2b6cb0"
            }
        },
        {
            "scope": [
                "meta.object-literal.value",
                "variable.other.readwrite",
                "variable.other.constant"
            ],
            "settings": {
                "foreground": "#2a724f"
            }
        },
        {
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#a0aec0",
                "fontStyle": "italic"
            }
        }
    ]
}